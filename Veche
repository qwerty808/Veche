            —Å–æ–∫–µ—Ç_–±–æ–≥–∞—Ç—ã—Ä—è.send(self._–∑–∞—à–∏—Ñ—Ä–æ–≤–∞—Ç—å_–ø–æ—Å–ª–∞–Ω–∏–µ(
                –±–æ–≥–∞—Ç—ã—Ä—å_id,
                {'—Ç–∏–ø': '—Å–∏—Å—Ç–µ–º–∞', '–¥–∞–Ω–Ω—ã–µ': f"üéµ –í–µ—á–µ–≤–∞—è –ø–µ—Å–Ω—è: {–ø–µ—Å–Ω—è}"}
            ))
            
            # –ì–ª–∞–≤–Ω—ã–π —Ü–∏–∫–ª –æ–±—â–µ–Ω–∏—è
            while True:
                –ø–æ—Å–ª–∞–Ω–∏–µ = —Å–æ–∫–µ—Ç_–±–æ–≥–∞—Ç—ã—Ä—è.recv(4096)
                if not –ø–æ—Å–ª–∞–Ω–∏–µ:
                    break
                    
                —Ä–∞—Å—à–∏—Ñ—Ä–æ–≤–∞–Ω–Ω–æ–µ = self._—Ä–∞—Å—à–∏—Ñ—Ä–æ–≤–∞—Ç—å_–ø–æ—Å–ª–∞–Ω–∏–µ(–±–æ–≥–∞—Ç—ã—Ä—å_id, –ø–æ—Å–ª–∞–Ω–∏–µ)
                if —Ä–∞—Å—à–∏—Ñ—Ä–æ–≤–∞–Ω–Ω–æ–µ:
                    self._–æ–±—Ä–∞–±–æ—Ç–∞—Ç—å_–ø–æ—Å–ª–∞–Ω–∏–µ(–±–æ–≥–∞—Ç—ã—Ä—å_id, —Ä–∞—Å—à–∏—Ñ—Ä–æ–≤–∞–Ω–Ω–æ–µ)
                    
        except Exception as –µ:
            print(f"‚ö° –û—à–∏–±–∫–∞ —Å –±–æ–≥–∞—Ç—ã—Ä–µ–º: {–µ}")
        finally:
            self._—É–¥–∞–ª–∏—Ç—å_–±–æ–≥–∞—Ç—ã—Ä—è(–±–æ–≥–∞—Ç—ã—Ä—å_id)
    
    def _–æ–±—Ä–∞–±–æ—Ç–∞—Ç—å_–ø–æ—Å–ª–∞–Ω–∏–µ(self, –±–æ–≥–∞—Ç—ã—Ä—å_id, –¥–∞–Ω–Ω—ã–µ):
        """–û–±—Ä–∞–±–æ—Ç–∞—Ç—å –ø–æ–ª—É—á–µ–Ω–Ω–æ–µ –ø–æ—Å–ª–∞–Ω–∏–µ"""
        —Ç–∏–ø_–ø–æ—Å–ª–∞–Ω–∏—è = –¥–∞–Ω–Ω—ã–µ.get('—Ç–∏–ø')
        —Å–æ–¥–µ—Ä–∂–∞–Ω–∏–µ = –¥–∞–Ω–Ω—ã–µ.get('–¥–∞–Ω–Ω—ã–µ', '').strip()
        –∏–º—è_–±–æ–≥–∞—Ç—ã—Ä—è = self.–±–æ–≥–∞—Ç—ã—Ä–∏[–±–æ–≥–∞—Ç—ã—Ä—å_id]['–∏–º—è']
        
        if —Ç–∏–ø_–ø–æ—Å–ª–∞–Ω–∏—è == '–≤–µ—á–µ–≤–æ–µ':
            # –í–µ—á–µ–≤–æ–µ (–ø—É–±–ª–∏—á–Ω–æ–µ) –ø–æ—Å–ª–∞–Ω–∏–µ
            if —Å–æ–¥–µ—Ä–∂–∞–Ω–∏–µ:
                formatted_time = datetime.now().strftime("%H:%M")
                –ø–æ—Å–ª–∞–Ω–∏–µ_–¥–ª—è_–≤–µ—á–µ = f"üïê {formatted_time} | üë§ {–∏–º—è_–±–æ–≥–∞—Ç—ã—Ä—è}: {—Å–æ–¥–µ—Ä–∂–∞–Ω–∏–µ}"
                
                self._–æ–ø–æ–≤–µ—Å—Ç–∏—Ç—å_–≤–µ—á–µ(–ø–æ—Å–ª–∞–Ω–∏–µ_–¥–ª—è_–≤–µ—á–µ, –∏—Å–∫–ª—é—á–∏—Ç—å=–±–æ–≥–∞—Ç—ã—Ä—å_id)
                
                # –ó–∞–ø–∏—Å—ã–≤–∞–µ–º –≤ —Å–æ–∫—Ä–æ–≤–∏—â–Ω–∏—Ü—É
                self.—Å–æ–∫—Ä–æ–≤–∏—â–Ω–∏—Ü–∞.append({
                    '–≤—Ä–µ–º—è': datetime.now().strftime("%H:%M:%S"),
                    '–æ—Ç': –∏–º—è_–±–æ–≥–∞—Ç—ã—Ä—è,
                    '–∫–æ–º—É': '–≤–µ—á–µ',
                    '–ø–æ—Å–ª–∞–Ω–∏–µ': —Å–æ–¥–µ—Ä–∂–∞–Ω–∏–µ,
                    '—Ç–∏–ø': '–≤–µ—á–µ–≤–æ–µ'
                })
                
                # –ò–Ω–æ–≥–¥–∞ –¥–æ–±–∞–≤–ª—è–µ–º —Ä–µ–∞–∫—Ü–∏—é –≤–µ—á–µ
                if random.random() < 0.1:  # 10% chance
                    —Ä–µ–∞–∫—Ü–∏–∏ = ["–í–µ—Ä–Ω–æ –≥–æ–≤–æ—Ä–∏—à—å!", "–°–ª–æ–≤–æ –º–æ–ª–≤–∏–ª!", "–¢–∞–∫ –∏ –±—ã—Ç—å –Ω–∞–¥–æ–±–Ω–æ!", "–°–æ–≥–ª–∞—Å–µ–Ω!"]
                    self._–æ–ø–æ–≤–µ—Å—Ç–∏—Ç—å_–≤–µ—á–µ(f"–í–µ—á–µ: {random.choice(—Ä–µ–∞–∫—Ü–∏–∏)}")
        
        elif —Ç–∏–ø_–ø–æ—Å–ª–∞–Ω–∏—è == '—Ç–∞–π–Ω–æ–µ':
            # –¢–∞–π–Ω–æ–µ (–ø—Ä–∏–≤–∞—Ç–Ω–æ–µ) –ø–æ—Å–ª–∞–Ω–∏–µ
            –∫–æ–º—É = –¥–∞–Ω–Ω—ã–µ.get('–∫–æ–º—É')
            –ø–æ—Å–ª–∞–Ω–∏–µ = –¥–∞–Ω–Ω—ã–µ.get('–ø–æ—Å–ª–∞–Ω–∏–µ')
            if –∫–æ–º—É and –ø–æ—Å–ª–∞–Ω–∏–µ:
                self._–æ—Ç–ø—Ä–∞–≤–∏—Ç—å_—Ç–∞–∏–Ω—Å—Ç–≤–µ–Ω–Ω–æ(–∏–º—è_–±–æ–≥–∞—Ç—ã—Ä—è, –∫–æ–º—É, –ø–æ—Å–ª–∞–Ω–∏–µ)
        
        elif —Ç–∏–ø_–ø–æ—Å–ª–∞–Ω–∏—è == '–∑–∞–ø—Ä–æ—Å':
            # –ó–∞–ø—Ä–æ—Å—ã
            if —Å–æ–¥–µ—Ä–∂–∞–Ω–∏–µ == '/–±–æ–≥–∞—Ç—ã—Ä–∏':
                self._–æ—Ç–ø—Ä–∞–≤–∏—Ç—å_—Å–ø–∏—Å–æ–∫_–±–æ–≥–∞—Ç—ã—Ä–µ–π(–±–æ–≥–∞—Ç—ã—Ä—å_id)
            elif —Å–æ–¥–µ—Ä–∂–∞–Ω–∏–µ == '/–ø–æ–º–æ—â—å':
                –ø–æ–º–æ—â—å = """üìú –°–í–ò–¢–û–ö –ó–ù–ê–ù–ò–ô:
/–±–æ–≥–∞—Ç—ã—Ä–∏ - –°–ø–∏—Å–æ–∫ –±–æ–≥–∞—Ç—ã—Ä–µ–π –Ω–∞ –≤–µ—á–µ
/—Ç–∞–π–Ω–æ [–∏–º—è] [–ø–æ—Å–ª–∞–Ω–∏–µ] - –®—ë–ø–æ—Ç –±–æ–≥–∞—Ç—ã—Ä—é
/—Å–∫–∞–ª—å–¥ - –í–µ—á–µ–≤–∞—è –ø–µ—Å–Ω—è
/–∏—Å—Ç–æ—Ä–∏—è - –ü–æ—Å–ª–µ–¥–Ω–∏–µ –ø–æ—Å–ª–∞–Ω–∏—è
/–ø—Ä–æ—á—å - –ü–æ–∫–∏–Ω—É—Ç—å –≤–µ—á–µ"""
                self.–±–æ–≥–∞—Ç—ã—Ä–∏[–±–æ–≥–∞—Ç—ã—Ä—å_id]['—Å–æ–∫–µ—Ç'].send(
                    self._–∑–∞—à–∏—Ñ—Ä–æ–≤–∞—Ç—å_–ø–æ—Å–ª–∞–Ω–∏–µ(
                        –±–æ–≥–∞—Ç—ã—Ä—å_id,
                        {'—Ç–∏–ø': '—Å–∏—Å—Ç–µ–º–∞', '–¥–∞–Ω–Ω—ã–µ': –ø–æ–º–æ—â—å}
                    )
                )
            elif —Å–æ–¥–µ—Ä–∂–∞–Ω–∏–µ == '/—Å–∫–∞–ª—å–¥':
                –ø–µ—Å–Ω—è = random.choice(self.–≤–µ—á–µ–≤—ã–µ_–ø–µ—Å–Ω–∏)
                self.–±–æ–≥–∞—Ç—ã—Ä–∏[–±–æ–≥–∞—Ç—ã—Ä—å_id]['—Å–æ–∫–µ—Ç'].send(
                    self._–∑–∞—à–∏—Ñ—Ä–æ–≤–∞—Ç—å_–ø–æ—Å–ª–∞–Ω–∏–µ(
                        –±–æ–≥–∞—Ç—ã—Ä—å_id,
                        {'—Ç–∏–ø': '—Å–∏—Å—Ç–µ–º–∞', '–¥–∞–Ω–Ω—ã–µ': f"üéµ –°–∫–∞–ª—å–¥ –≤–µ—â–∞–µ—Ç: {–ø–µ—Å–Ω—è}"}
                    )
                )
            elif —Å–æ–¥–µ—Ä–∂–∞–Ω–∏–µ == '/–∏—Å—Ç–æ—Ä–∏—è':
                –∏—Å—Ç–æ—Ä–∏—è = self._–ø–æ–ª—É—á–∏—Ç—å_–∏—Å—Ç–æ—Ä–∏—é()
                self.–±–æ–≥–∞—Ç—ã—Ä–∏[–±–æ–≥–∞—Ç—ã—Ä—å_id]['—Å–æ–∫–µ—Ç'].send(
                    self._–∑–∞—à–∏—Ñ—Ä–æ–≤–∞—Ç—å_–ø–æ—Å–ª–∞–Ω–∏–µ(
                        –±–æ–≥–∞—Ç—ã—Ä—å_id,
                        {'—Ç–∏–ø': '—Å–∏—Å—Ç–µ–º–∞', '–¥–∞–Ω–Ω—ã–µ': –∏—Å—Ç–æ—Ä–∏—è}
                    )
                )
    
    def _–æ—Ç–ø—Ä–∞–≤–∏—Ç—å_—Å–ø–∏—Å–æ–∫_–±–æ–≥–∞—Ç—ã—Ä–µ–π(self, –±–æ–≥–∞—Ç—ã—Ä—å_id):
        """–û—Ç–ø—Ä–∞–≤–∏—Ç—å —Å–ø–∏—Å–æ–∫ –±–æ–≥–∞—Ç—ã—Ä–µ–π –Ω–∞ –≤–µ—á–µ"""
        with self.–±–ª–æ–∫–∏—Ä–æ–≤–∫–∞:
            –∏–º–µ–Ω–∞ = [–∏–Ω—Ñ–æ['–∏–º—è'] for –∏–Ω—Ñ–æ in self.–±–æ–≥–∞—Ç—ã—Ä–∏.values()]
            —Å–ø–∏—Å–æ–∫ = "üèπ –ë–æ–≥–∞—Ç—ã—Ä–∏ –Ω–∞ –≤–µ—á–µ: " + ", ".join(–∏–º–µ–Ω–∞)
            
            if –±–æ–≥–∞—Ç—ã—Ä—å_id in self.–±–æ–≥–∞—Ç—ã—Ä–∏:
                self.–±–æ–≥–∞—Ç—ã—Ä–∏[–±–æ–≥–∞—Ç—ã—Ä—å_id]['—Å–æ–∫–µ—Ç'].send(
                    self._–∑–∞—à–∏—Ñ—Ä–æ–≤–∞—Ç—å_–ø–æ—Å–ª–∞–Ω–∏–µ(
                        –±–æ–≥–∞—Ç—ã—Ä—å_id,
                        {'—Ç–∏–ø': '—Å–∏—Å—Ç–µ–º–∞', '–¥–∞–Ω–Ω—ã–µ': —Å–ø–∏—Å–æ–∫}
                    )
                )
    
    def _–ø–æ–ª—É—á–∏—Ç—å_–∏—Å—Ç–æ—Ä–∏—é(self):
        """–ü–æ–ª—É—á–∏—Ç—å –ø–æ—Å–ª–µ–¥–Ω–∏–µ –ø–æ—Å–ª–∞–Ω–∏—è –∏–∑ —Å–æ–∫—Ä–æ–≤–∏—â–Ω–∏—Ü—ã"""
        if not self.—Å–æ–∫—Ä–æ–≤–∏—â–Ω–∏—Ü–∞:
            return "–°–æ–∫—Ä–æ–≤–∏—â–Ω–∏—Ü–∞ –≤–µ—á–µ –ø—É—Å—Ç–∞"
        
        –∏—Å—Ç–æ—Ä–∏—è = "üìú –ü–û–°–õ–ï–î–ù–ò–ï –ü–û–°–õ–ê–ù–ò–Ø:\n"
        –ø–æ—Å–ª–µ–¥–Ω–∏–µ = self.—Å–æ–∫—Ä–æ–≤–∏—â–Ω–∏—Ü–∞[-10:]  # –ü–æ—Å–ª–µ–¥–Ω–∏–µ 10 —Å–æ–æ–±—â–µ–Ω–∏–π
        for –∑–∞–ø–∏—Å—å in –ø–æ—Å–ª–µ–¥–Ω–∏–µ:
            if –∑–∞–ø–∏—Å—å['—Ç–∏–ø'] == '–≤–µ—á–µ–≤–æ–µ':
                –∏—Å—Ç–æ—Ä–∏—è += f"{–∑–∞–ø–∏—Å—å['–≤—Ä–µ–º—è']} üë§ {–∑–∞–ø–∏—Å—å['–æ—Ç']}: {–∑–∞–ø–∏—Å—å['–ø–æ—Å–ª–∞–Ω–∏–µ']}\n"
            else:
                –∏—Å—Ç–æ—Ä–∏—è += f"{–∑–∞–ø–∏—Å—å['–≤—Ä–µ–º—è']} ‚ú® {–∑–∞–ø–∏—Å—å['–æ—Ç']} ‚Üí {–∑–∞–ø–∏—Å—å['–∫–æ–º—É']}: {–∑–∞–ø–∏—Å—å['–ø–æ—Å–ª–∞–Ω–∏–µ']}\n"
        
        return –∏—Å—Ç–æ—Ä–∏—è
    
    def _—É–¥–∞–ª–∏—Ç—å_–±–æ–≥–∞—Ç—ã—Ä—è(self, –±–æ–≥–∞—Ç—ã—Ä—å_id):
        """–ë–æ–≥–∞—Ç—ã—Ä—å –ø–æ–∫–∏–¥–∞–µ—Ç –≤–µ—á–µ"""
        with self.–±–ª–æ–∫–∏—Ä–æ–≤–∫–∞:
            if –±–æ–≥–∞—Ç—ã—Ä—å_id in self.–±–æ–≥–∞—Ç—ã—Ä–∏:
                –∏–º—è = self.–±–æ–≥–∞—Ç—ã—Ä–∏[–±–æ–≥–∞—Ç—ã—Ä—å_id]['–∏–º—è']
                self.–±–æ–≥–∞—Ç—ã—Ä–∏[–±–æ–≥–∞—Ç—ã—Ä—å_id]['—Å–æ–∫–µ—Ç'].close()
                del self.–±–æ–≥–∞—Ç—ã—Ä–∏[–±–æ–≥–∞—Ç—ã—Ä—å_id]
                
                if –±–æ–≥–∞—Ç—ã—Ä—å_id in self.–∫–ª—é—á–∏:
                    del self.–∫–ª—é—á–∏[–±–æ–≥–∞—Ç—ã—Ä—å_id]
                
                –ø—Ä–æ—â–∞–Ω–∏—è = [
                    f"{–∏–º—è} –ø–æ–∫–∏–Ω—É–ª –≤–µ—á–µ. –î–∞ —Ö—Ä–∞–Ω—è—Ç –µ–≥–æ –±–æ–≥–∏!",
                    f"{–∏–º—è} –æ—Ç–±—ã–ª. –°–ª–æ–≤–æ –µ–≥–æ –ø–æ–º–Ω–∏—Ç—å –±—É–¥–µ–º!",
                    f"–ü—Ä–æ—â–∞–π, {–∏–º—è}! –í–æ–∑–≤—Ä–∞—â–∞–π—Å—è –Ω–∞ –≤–µ—á–µ!",
                    f"{–∏–º—è} —Å–∫—Ä—ã–ª—Å—è –≤ —Ç—É–º–∞–Ω–µ..."
                ]
                
                print(f"üë£ {–∏–º—è} –ø–æ–∫–∏–Ω—É–ª –≤–µ—á–µ")
                self._–æ–ø–æ–≤–µ—Å—Ç–∏—Ç—å_–≤–µ—á–µ(random.choice(–ø—Ä–æ—â–∞–Ω–∏—è))
    
    def –Ω–∞—á–∞—Ç—å(self):
        """–ù–∞—á–∞—Ç—å —Å–ª—É—à–∞—Ç—å –≤–µ—á–µ"""
        print("–í–µ—á–µ –Ω–∞—á–∏–Ω–∞–µ—Ç —Å–ª—É—à–∞—Ç—å –±–æ–≥–∞—Ç—ã—Ä–µ–π...")
        while True:
            try:
                —Å–æ–∫–µ—Ç_–±–æ–≥–∞—Ç—ã—Ä—è, –∞–¥—Ä–µ—Å = self.—Å–µ—Ä–≤–µ—Ä.accept()
                –Ω–∏—Ç—å = threading.Thread(
                    target=self._–ø—Ä–∏–Ω—è—Ç—å_–±–æ–≥–∞—Ç—ã—Ä—è,
                    args=(—Å–æ–∫–µ—Ç_–±–æ–≥–∞—Ç—ã—Ä—è, –∞–¥—Ä–µ—Å)
                )
                –Ω–∏—Ç—å.daemon = True
                –Ω–∏—Ç—å.start()
            except KeyboardInterrupt:
                print("\n–í–µ—á–µ –∑–∞–≤–µ—Ä—à–∞–µ—Ç —Ä–∞–±–æ—Ç—É...")
                break

if __name__ == "__main__":
    print("""
‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢Ä‚£Ä‚£Ä‚£Ä‚£Ä‚£Ä‚£Ä‚£Ä‚°Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä
‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢Ä‚£§‚£∂‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£∂‚£§‚°Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä
